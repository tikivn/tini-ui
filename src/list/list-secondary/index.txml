<view class="tu-list-secondary">
  <auto-sizer tiki:if="{{thumb}}" 
    placeholder="{{thumbPlaceholder}}" 
    src="{{thumb}}" 
    size="{{thumbSize}}" />
  <view class="tu-list-secondary-text">
    <view class="tu-list-secondary-title" tiki:if="{{title}}">{{title}}</view>
    <view class="tu-list-secondary-subtitle" tiki:if="{{subtitle}}">{{subtitle}}</view>
  </view>
</view>