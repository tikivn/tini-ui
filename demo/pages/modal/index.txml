<modal
  show="{{modalOpened23}}"
  onModalClick="onModalClick23"
  onModalClose="onModalClose23"
  showClose="{{true}}"
  topImage="https://picsum.photos/id/866/400/400.jpg"
>
  <view slot="header">Icon</view>
  Modal icon
  <tm-icon type="success" color="red" />
  <view slot="footer">Footer</view>
</modal>
<modal
  show="{{modalOpened}}"
  onModalClick="onModalClick"
  onModalClose="onModalClose"
>
  <view style="margin: 20px 0 10px 0;">This is a image</view>
  <image mode="widthFix" style="width: 100%;" src="https://picsum.photos/id/866/400/400.jpg" />
  Content
  <view slot="footer">Footer</view>
</modal>
<modal
  show="{{modalOpened3}}"
  onModalClose="onModalClick3"
  advice="{{true}}"
  >
  <view style="display: flex; height: 280px; width: 280px;">
    <image
      style="height: 280px; width: 280px;"
      src="https://picsum.photos/id/866/400/400.jpg"
    />
  </view>
</modal>
<modal
  show="{{modalOpened4}}"
  onModalClose="onModalClick4"
  advice="{{true}}"
  >
  <view style="display: flex; width: 270px; height: 350px;">
    <image
      style="width: 270px; height: 350px;"
      src="https://picsum.photos/id/866/400/400.jpg"
    />
  </view>
</modal>
<modal
  show="{{modalOpened5}}"
  showClose="{{false}}"
  onButtonClick="onButtonClick5"
  buttons="{{buttons5}}"
>
  <view slot="header">Header</view>
  Content
</modal>
<modal
  show="{{modalOpened6}}"
  showClose="{{false}}"
  onButtonClick="onButtonClick6"
  buttonsLayout="vertical"
  buttons="{{buttons6}}"
>
  <view slot="header">Header</view>
  Buttons 6
</modal>
<modal
  show="{{modalOpened7}}"
