<button onTap="openModal21">Image</button>
<button onTap="openModal22">Image sm</button>
<button onTap="openModal222">Image md</button>
<button onTap="openModal2">Image lg</button>
<button onTap="openModal23">Icon</button>
<button onTap="openModal">Lock Mask</button>
<button onTap="openModal3">Advice</button>
<button onTap="openModal4">Advice small</button>
<button onTap="openModal5">Button horizontal</button>
<button onTap="openModal6">Button vertical</button>
<button onTap="openModal7">Custom button style</button>
<button onTap="openModal8">3 Buttons vertical</button>
<button onTap="openModal9">User Buton</button>

<modal
  show="{{modalOpened21}}"
  onMaskClick="onMaskClick21"
  onModalClick="onModalClick21"
  onModalClose="onModalClose21"
  topImage="https://picsum.photos/id/866/400/400.jpg"
>
  <view slot="header">Image default is "md"</view>
  Modal content
  <view slot="footer">Footer</view>
</modal>
<modal
  show="{{modalOpened22}}"
  onModalClick="onModalClick22"
  onModalClose="onModalClose22"
