<modal
  show="{{modalOpened9}}"
  showClose="{{false}}"
>
  <view slot="header">Header</view>
  <view>Content</view>
  <view style="margin-top: 10px; padding-bottom: 10px;">
    <!-- <slider value="5" step="5"/> -->
  </view>
  <button type="primary" onTap="onModalClose9">Close</button>
</modal>
